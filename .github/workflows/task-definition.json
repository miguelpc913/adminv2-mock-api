{
    "containerDefinitions": [
        {
            "name": "admin-mock-api",
            "image": "227320912480.dkr.ecr.eu-west-1.amazonaws.com/admin-mock-api:develop",
            "cpu": 1,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": ["CMD-SHELL","curl -f http://localhost:8080/health || exit 1"],
                "interval": 60,
                "timeout": 5,
                "retries": 3
            },
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "family": "admin-mock-api"
}